#include <iostream>
using namespace std;
int main()
{
    int n,total=0,x,y;
    cin>>n;
    while(n>0)
    {
        x=n%10;
        n=n/10;
        switch(x)
        {
            case 0:
                y=99;
                break;
            case 1:
                y=88;
                break;
            case 2:
                y=77;
                break;
            case 3:
                y=66;
                break;
            case 4:
                y=55;
                break;
            case 5:
                y=44;
                break;
            case 6:
                y=33;
                break;
            case 7:
                y=22;
                break;
            case 8:
                y=11;
                break;
            case 9:
                y=0;
                break; 
        }
        total=total+y;
    }
    cout<<total;
}
