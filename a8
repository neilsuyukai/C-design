#include <iostream>
using namespace std;
int main()
{
    int a,b,c;
    cin>>a>>b>>c;
    if(a*a==b*b+c*c||b*b==a*a+c*c||c*c==a*a+b*b)
    {
        cout<<"It is a right triangle";
        return 0;
    }
    else if(a<=0||b<=0||c<=0)
    {
        cout<<"Not a triangle";
        return 0;
    }
    if(a+b>c||b+c>a||a+c>b)
    {
    cout<<"It is not a right triangle";
    return 0;
    }
    else if(a+b<=c||b+c<=a||a+c<=b)
    {
    cout<<"Not a triangle";
    return 0;
    }
}
