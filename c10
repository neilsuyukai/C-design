#include <iostream>
using namespace std;
int main()
{
    int x,y;
    cin>>x>>y;
    switch(x)
    {
        case 1:
        for(int i=1;i<=y;i++)
        {
            for(int j=1;j<=y;j++)
            {
                if(i>=j)
                    cout<<"*";
                else 
                    cout<<"-";
            }
            cout<<endl;
        }
         break;
        case 2:
        for(int i=1;i<=y;i++)
        {
            for(int j=1;j<=y;j++)
            {
                if(i+j<=y+1)
                    cout<<"*";
                else 
                    cout<<"-";
            }
            cout<<endl;
        }
         break;
        case 3:
        for(int i=1;i<=y;i++)
        {
            for(int j=1;j<=y;j++)
            {
                if(i+j>=y+1)
                    cout<<"*";
                else 
                    cout<<"-";
            }
            cout<<endl;
        }
         break;
        case 4:
        for(int i=1;i<=y;i++)
        {
            for(int j=1;j<=y;j++)
            {
                if(i<=j)
                    cout<<"*";
                else 
                    cout<<"-";
            }
            cout<<endl;
        }
        break;
    }
}
