#include<iostream>
using namespace std;
int integerPower(int base,int exp);
int main()
{
   int exp; 
   int base; 
   
   cin >> base >> exp;
   cout << integerPower( base, exp ) << endl;
}
int integerPower(int base,int exp)
{
    int ans=1;
    for(int i=1;i<=exp;i++)
    {
        ans = ans*base;
    }
    return ans; 
}
