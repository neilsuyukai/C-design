#include <iostream>
using namespace std;
int main()
{
    int n,total=0,m,integer;
    cin>>n;
    while(n>0)
    {
        m=n%10;
        switch(m)
        {
            case 1:
                integer=+9;
                break;
            case 2:
                integer=+8;
                break;
            case 3:
                integer=+7;
                break;
            case 4:
                integer=+6;
                break;
            case 5:
                integer=+5;
                break;
            case 6:
                integer=+4;
                break;
            case 7:
                integer=+3;
                break;
            case 8:
                integer=+2;
                break;
            case 9:
                integer=+1;
                break;
    }
    if(n%10==0)
    {
        total=total*-1;
    }
    else 
        total=total+integer;
        n=n/10;
    }
    cout<<total;
}
